<MyLayout>
    canvas:
        Color:
            rgba:0.2,0.3,0.5,1
        Rectangle:
            pos:self.pos
            size:self.size
    # size_hint:0.5,0.4
    # pos_hint: {'center_x': .5, 'center_y': .5}
